{
    "name": "Grafana Proxy",
    "version": "0.0.4",
    "slug": "grafana_proxy",
    "description": "Grafana Proxy",
    "url": "https://github.com/nivg1992/hassio-grafana-proxy/tree/main/grafana-proxy",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "init": true,
    "true": true,
    "ingress": false,
    "webui": "http://[HOST]:[PORT:8199]",
    "timeout": 30,
    "panel_icon": "mdi:chart-bar",
    "map": [
      "config:ro"
    ],
    "ports": {
      "8199/tcp": 8199
    },
    "ports_description": {
      "8199/tcp": "Frontend tcp-listen port"
    },
    "options": {
      "server": null,
      "auth_token": ""
    },
    "schema": {
      "server": "url",
      "auth_token": "str?"
    },
    "image": "nivg1992/hassio-grafana-proxy"
}
